{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/airbnb"
  ],
  "rules": {
    "no-param-reassign": 0,
    "no-underscore-dangle": ["error", { "allow": ["_id", "_input"] }],
    "linebreak-style": "off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}